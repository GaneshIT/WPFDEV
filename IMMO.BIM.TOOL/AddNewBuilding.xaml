<Window x:Class="IMMO.BIM.TOOL.AddNewBuilding"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IMMO.BIM.TOOL"
        mc:Ignorable="d"
        Title="New Building" Height="450" Width="800">
    <Grid>
        <Label Content="Gebaude ID :" HorizontalAlignment="Left" Margin="200,167,0,0" VerticalAlignment="Top" Width="110" Height="35"/>
        <TextBox Name="txtBuildingId" HorizontalAlignment="Left" Height="35" Margin="353,167,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="286"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="200,233,0,0" VerticalAlignment="Top" Width="110" Click="BtnCancel_Click" Height="40"/>
        <Button Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="353,233,0,0" VerticalAlignment="Top" Width="111" RenderTransformOrigin="-1.021,0.592" Click="BtnAdd_Click" Height="40"/>
        <Label Content="New Building" HorizontalAlignment="Center" Margin="200,90,0,0" VerticalAlignment="Top" Width="439" Height="56" FontSize="20"/>

    </Grid>
</Window>
