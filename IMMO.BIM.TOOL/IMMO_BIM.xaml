        <ListBox Name="lbDisplayLevels" HorizontalAlignment="Left" Height="422" Margin="312,90,0,0" VerticalAlignment="Top" Width="142"/>
        <ListBox Name="lbDisplayRooms" HorizontalAlignment="Left" Height="422" Margin="550,90,0,0" VerticalAlignment="Top" Width="142"/>
        <Button Content="Edit Room" HorizontalAlignment="Left" Margin="843,462,0,0" VerticalAlignment="Top" Width="162" Name="btnEditRoom" Click="BtnEditRoom_Click" />
        <Button Name="btnDeleteRoom" Content="Delete Room" HorizontalAlignment="Left" Margin="843,496,0,0" VerticalAlignment="Top" Width="162" Height="22" Click="BtnDeleteRoom_Click"/>
        <Button Name="btnExport" Content="Export" HorizontalAlignment="Left" Margin="843,538,0,0" VerticalAlignment="Top" Width="162" Click="BtnExport_Click"/>

    </Grid>
</Window>
