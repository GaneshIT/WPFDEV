<Window x:Class="IMMO.BIM.TOOL.IMMO_BIM"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IMMO.BIM.TOOL"
        mc:Ignorable="d"
        Title="IMMO_BIM" Height="684.771" Width="1364.735">
    <Grid>
        <Label Content="Building" HorizontalAlignment="Left" Margin="64,59,0,0" VerticalAlignment="Top" Width="90"/>
        <Label Content="Levels" HorizontalAlignment="Left" Margin="312,59,0,0" VerticalAlignment="Top" Width="90"/>
        <Label Content="Rooms" HorizontalAlignment="Left" Margin="550,59,0,0" VerticalAlignment="Top" Width="90"/>
        <Label Content="Info" HorizontalAlignment="Left" Margin="843,59,0,0" VerticalAlignment="Top" Width="90"/>
        <Button Name="btnNewBuilding" Content="New Building" HorizontalAlignment="Left" Margin="64,538,0,0" VerticalAlignment="Top" Width="154" Click="BtnNewBuilding_Click"/>
        <Button Name="btnNewRoom" Content="New room" HorizontalAlignment="Left" Margin="550,538,0,0" VerticalAlignment="Top" Width="154" RenderTransformOrigin="0.66,-0.536" Click="BtnNewRoom_Click"/>
        <Button Name="btnAddLevel" Content="Add level" HorizontalAlignment="Left" Margin="312,538,0,0" VerticalAlignment="Top" Width="154" Click="BtnAddLevel_Click"/>
